{
  "compilerOptions": {
    "target": "es3",
    "module": "none",
    "lib": ["ESNext"],
    "outFile": "../index.js",
    "downlevelIteration": true,
    "strict": true,
    "strictNullChecks": true,
    "types": [
      "windows-script-host",
      "activex-scripting"
    ],
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  },
  "include": ["./*.ts"]
}
